<div class="panel panel-default">
  <div class="panel-heading">
    <div class="row">
      <div class="col-md-2">担保人</div>
      <div class="col-md-2 col-md-offset-8">
        <button type="button" class="btn btn-primary btn-block btn-xs" data-toggle="modal" data-target="#new-guarantor-modal">添加担保人</button>
      </div>
    </div>
  </div>
  <div class="panel-body">
    <%- partial('modals/guarantor') %>
    <table class="table table-condensed table-hover" id="guarantors">
      <thead>
        <tr>
          <th>名称</th>
          <th>法人代表</th>
          <th>注册资本</th>
          <th>总资产</th>
          <th>净资产</th>
          <th>对外担保金额</th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
  </div>
</div>
